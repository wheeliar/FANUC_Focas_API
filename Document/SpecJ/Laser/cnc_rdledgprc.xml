            <fs21ib>-</fs21ib>
            <fs30ia>-</fs30ia>
            <fs30ib>-</fs30ib>
        </m>
        <!-- T（旋盤） -->
        <t>
            <fs0ia>-</fs0ia>
            <fs0ib>-</fs0ib>
            <fs0id>-</fs0id>
            <fs0if>-</fs0if>
            <fs15>-</fs15>
            <fs15i>-</fs15i>
            <fs16>-</fs16>
            <fs18>-</fs18>
            <fs21>-</fs21>
            <fs16ia>-</fs16ia>
            <fs18ia>-</fs18ia>
            <fs21ia>-</fs21ia>
            <fs16ib>-</fs16ib>
            <fs18ib>-</fs18ib>
            <fs21ib>-</fs21ib>
            <fs30ia>-</fs30ia>
            <fs30ib>-</fs30ib>
        </t>
        <!-- LC（ローダ制御） -->
        <lc>
            <fs0ia>-</fs0ia>
            <fs0ib>-</fs0ib>
            <fs0id>-</fs0id>
            <fs0if>-</fs0if>
            <fs15>-</fs15>
            <fs15i>-</fs15i>
            <fs16>-</fs16>
            <fs18>-</fs18>
            <fs21>-</fs21>
            <fs16ia>-</fs16ia>
            <fs18ia>-</fs18ia>
